{% if theme == 'gruvbox' %}
[colors]
darkgray = #1d2021 
orange = #d65d0e
white = #ebdbb2
gray = #585858
black = #090909
red = #c795ae
blue = #95aec7
yellow = #c7ae95
green = #aec795
background = #000000 
background-alt = #4e4e4e
foreground = #ebdbb2 
foreground-alt = #4e4e4e
primary = #d79921 
secondary = #FF6200
alert = #fb4934
disabled = #000000

[bar/example]
width = 100%
height = 20pt

; dpi = 96

background = ${colors.background}
foreground = ${colors.foreground}

line-size = 3pt

border-size = 4pt
border-color = #000000

padding-left = 0
padding-right = 0

module-margin = 1

separator = 
separator-foreground = ${colors.disabled}

font-0 = monospace:size=8;2
font-1 = Terminess Nerd Font:size=8;2
font-2 = Noto Color Emoji:scale=15;
modules-left = xworkspaces
modules-center = wlan eth power run help
modules-right ={% if system_type == 'laptop' %} backlight battery{% endif %} btc filesystem cpu memory pulseaudio date

cursor-click = pointer
cursor-scroll = ns-resize

enable-ipc = true

tray-position = right

; wm-restack = generic
; wm-restack = bspwm
; wm-restack = i3

; override-redirect = true

[module/xworkspaces]
type = internal/xworkspaces

label-active = %name%
label-active-background = ${colors.background-alt}
label-active-underline= #cc241d 
label-active-padding = 1

label-occupied = %name%
label-occupied-padding = 1

label-urgent = %name%
label-urgent-background = ${colors.alert}
label-urgent-padding = 1

label-empty = %name%
label-empty-foreground = ${colors.disabled}
label-empty-padding = 1

[module/filesystem]
type = internal/fs
interval = 25

mount-0 = /

label-mounted = %{F#d79921}%mountpoint%%{F#d79921} Free: %free%

label-unmounted = %mountpoint% not mounted
label-unmounted-foreground = ${colors.disabled}
format-mounted-background = #282828
format-mounted-foreground = ${colors.primary}
format-mounted-padding = 1

[module/pulseaudio]
type = internal/pulseaudio

format-volume-prefix = "VOL "
format-volume-prefix-foreground = ${colors.primary}
format-volume = <label-volume>
format-volume-background = #282828
format-volume-foreground = ${colors.primary}
format-padding = 1
label-volume = %percentage%%

label-muted = üîá
label-muted-foreground = ${colors.disabled}

[module/xkeyboard]
type = internal/xkeyboard
blacklist-0 = num lock

label-layout = %layout%
label-layout-foreground = ${colors.primary}

label-indicator-padding = 2
label-indicator-margin = 1
label-indicator-foreground = ${colors.background}
label-indicator-background = ${colors.secondary}

[module/memory]
type = internal/memory
interval = 2
format-prefix = "MEM: "
format-prefix-foreground = ${colors.primary}
label = %gb_used:2%/%gb_total:2% (%percentage_used:2%%) 
format = <label> 
format-background = #282828
format-foreground = ${colors.primary}
format-padding = 1

[module/cpu]
type = internal/cpu
interval = 2
format-prefix = "CPU: "
label-cpu = %percentage:2%%
format = <label> 
format-background = #282828
format-foreground = ${colors.primary}
format-padding = 1

[network-base]
type = internal/network
interval = 5
format-connected = <label-connected>
format-disconnected = <label-disconnected>
label-disconnected = %{F#F0C674}%ifname%%{F#707880} disconnected
label-connected = %{F#d79921}%ifname:%%{F#d79921} ‚Üë %upspeed% ‚Üì %downspeed%
; format-connected = <label connected>
format-connected-background = #282828
format-connected-foreground = ${colors.primary}
; Add padding to make it look more like a button
format-connected-padding = 1
format-disconnected-background = #282828
format-disconnected-foreground = ${colors.primary}
format-disconnected-padding = 1

[module/wlan]
inherit = network-base
interface-type = wireless

[module/eth]
inherit = network-base
interface-type = wired

[module/date]
type = internal/date
interval = 1

date = %Y-%m-%d %H:%M:%S

label = %date%
label-foreground = ${colors.primary}
format = "%{A1:gsimplecal:} <label>%{A}"
format-background = #282828
format-foreground = ${colors.primary}
format-padding = 1

[module/run]
type = custom/text
content = "Run"
click-left = j4-dmenu-desktop --dmenu='dmenu -i -l 20 -nb "#282828" -nf "#ebdbb2" -sb "#fb4934" -sf "#fabd2f"' --term='xterm' 
; Set background and foreground colors to create the button effect
content-background = #282828
content-foreground = ${colors.primary}
; Add padding to make it look more like a button
content-padding = 1
; ; Use underline to simulate a border or shadow
content-underline = #ebdbb2
content-underline-size = 2

[module/power]
type = custom/text
content = "‚èª"
content-font = 2
click-left = xterm -e python3 /usr/local/bin/logoutmenu.py
; Set background and foreground colors to create the button effect
content-background = #282828
content-foreground = ${colors.primary}
; Add padding to make it look more like a button
content-padding = 1
; ; Use underline to simulate a border or shadow
content-underline = #ebdbb2
content-underline-size = 2

[module/help]
type = custom/text
content = "?"
click-left = xdg-open https://i3wm.org/docs/refcard.html 
; Set background and foreground colors to create the button effect
content-background = #282828
content-foreground = ${colors.primary}
; Add padding to make it look more like a button
content-padding = 1
; ; Use underline to simulate a border or shadow
content-underline = #ebdbb2
content-underline-size = 2

[module/btc]
type = custom/script
exec = curl -s https://api.coindesk.com/v1/bpi/currentprice/USD.json | jq -r '.bpi.USD.rate' | sed "s/\\..*//" 
interval = 150
format-prefix = "ÔÖö "
prefix-font = 2
; Set background and foreground colors to create the button effect
format-background = #282828
format-foreground = ${colors.primary}
; Add padding to make it look more like a button
format-padding = 1

{% if system_type == 'laptop' %}
[module/battery]
type = internal/battery
full-at = 99
low-at = 5
battery = BAT0
adapter = ADP1
poll-interval = 5
label-discharging = üîã %percentage%%
label-discharging-font = 2
format-discharging-background = #282828
format-discharging-foreground = ${colors.primary}
label-charging = üîå %percentage%%
label-charging-font = 2
format-charging-background = #282828
format-charging-foreground = ${colors.primary}

[module/backlight]
type = internal/backlight
card = acpi_video0
use-actual-brightness = true
poll-interval = 0
enable-scroll = true
scroll-interval = 10
format = <label> <ramp>
label = %percentage%%
format-background = #282828
format-foreground = ${colors.primary}
format-padding = 1
ramp-0 = üåï
ramp-1 = üåî
ramp-2 = üåì
ramp-3 = üåí
ramp-4 = üåë

{% endif %}
[settings]
screenchange-reload = true
pseudo-transparency = true

; vim:ft=dosini

